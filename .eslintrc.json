{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "ignorePatterns": "dist/**/*.js",
  "rules": {
    "prettier/prettier": ["error"],
    "default-case": "off",
    "import/no-cycle": "off",
    "no-param-reassign": "off",
    "no-nested-ternary": "off",
    "no-underscore-dangle": ["error", { "allow": ["_imageCache", "_retryStats"] }]
  }
}
